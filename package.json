{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm --filter openapi-spec-bundler build && pnpm i",
    "dev:docs": "pnpm --filter docs dev",
    "build:docs": "pnpm --filter docs... build",
    "prepreview:docs": "pnpm --filter docs build",
    "preview:docs": "pnpm --filter docs preview",
    "dev:react": "pnpm --filter react-remote dev",
    "build:react": "pnpm --filter react-remote... build",
    "prepreview:react": "pnpm --filter react-remote build",
    "preview:react": "pnpm --filter react-remote preview",
    "dev:todo-api": "pnpm --filter todo-api dev",
    "build:todo-openapi": "pnpm --filter todo-openapi... build",
    "build:openapi-spec-bundler": "pnpm --filter openapi-spec-bundler... build",
    "start:todo-angular": "pnpm --filter todo-angular start",
    "storybook:todo-angular": "pnpm --filter todo-angular storybook",
    "build-storybook:todo-angular": "pnpm --filter todo-angular build-storybook"
  }
}