{
  "name": "todo-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "init-msw": "msw init public/",
    "storybook": "ng run todo-angular:storybook",
    "prebuild-storybook": "pnpm init-msw",
    "build-storybook": "ng run todo-angular:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@azure/msal-angular": "4.0.20",
    "@azure/msal-browser": "4.24.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "^18.2.0",
    "@compodoc/compodoc": "^1.1.30",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/angular": "^9.1.5",
    "@storybook/theming": "8.6.14",
    "@types/jest": "^30.0.0",
    "@types/node": "24.3.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-preset-angular": "^15.0.0",
    "msw": "^2.11.2",
    "msw-storybook-addon": "^2.0.5",
    "storybook": "^9.1.5",
    "typescript": "~5.5.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}